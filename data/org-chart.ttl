@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://example.org/org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# Vocabulary definitions
org:hasPosition a rdf:Property ;
    rdfs:label "has position" ;
    rdfs:comment "3D position coordinates for visualization" .

org:belongsToDepartment a rdf:Property ;
    rdfs:label "belongs to department" ;
    rdfs:comment "Department membership" .

org:reportsTo a rdf:Property ;
    rdfs:label "reports to" ;
    rdfs:comment "Reporting relationship" .

org:manages a rdf:Property ;
    rdfs:label "manages" ;
    rdfs:comment "Management relationship" .

org:collaboratesWith a rdf:Property ;
    rdfs:label "collaborates with" ;
    rdfs:comment "Collaboration relationship" .

org:hasLevel a rdf:Property ;
    rdfs:label "has level" ;
    rdfs:comment "Organizational level" .

# Departments (positioned as layers)
org:engineering a org:Department ;
    foaf:name "Engineering" ;
    org:hasPosition "0,0,0" ;
    org:belongsToDepartment org:engineering ;
    org:hasLevel "0" .

org:product a org:Department ;
    foaf:name "Product" ;
    org:hasPosition "0,5,0" ;
    org:belongsToDepartment org:product ;
    org:hasLevel "5" .

org:design a org:Department ;
    foaf:name "Design" ;
    org:hasPosition "0,10,0" ;
    org:belongsToDepartment org:design ;
    org:hasLevel "10" .

org:executive a org:Department ;
    foaf:name "Executive" ;
    org:hasPosition "0,15,0" ;
    org:belongsToDepartment org:executive ;
    org:hasLevel "15" .


# Cross-layer mentorship relationships
sn:mentors a rdf:Property ;
    rdfs:label "mentors" ;
    rdfs:comment "Mentorship relationship" .

sn:jack sn:mentors sn:alice .
sn:karen sn:mentors sn:diana .
sn:grace sn:mentors sn:henry .
sn:alice sn:mentors sn:bob .
sn:diana sn:mentors sn:elena .

# Project collaborations
sn:collaboratesWith a rdf:Property ;
    rdfs:label "collaborates with" ;
    rdfs:comment "Active project collaboration" .

sn:alice sn:collaboratesWith sn:grace, sn:diana .

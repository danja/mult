@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sn: <http://example.org/social/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Social Network Vocabulary
sn:Person a rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person in the social network" .

sn:Community a rdfs:Class ;
    rdfs:label "Community" ;
    rdfs:comment "A community or group within the social network" .

sn:memberOf a rdf:Property ;
    rdfs:label "member of" ;
    rdfs:comment "Indicates community membership" .

sn:hasPosition a rdf:Property ;
    rdfs:label "has position" ;
    rdfs:comment "3D position for visualization" .

sn:hasInfluence a rdf:Property ;
    rdfs:label "has influence" ;
    rdfs:comment "Influence score in the network" .

sn:hasColor a rdf:Property ;
    rdfs:label "has color" ;
    rdfs:comment "Visual color for communities" .

sn:hasHeight a rdf:Property ;
    rdfs:label "has height" ;
    rdfs:comment "Layer height for communities" .

# Communities
sn:developers a sn:Community ;
    foaf:name "Developers" ;
    sn:hasPosition "0,0,0" ;
    sn:memberOf sn:developers ;
    sn:hasColor "0x4A90E2" ;
    sn:hasHeight "0" .

sn:designers a sn:Community ;
    foaf:name "Designers" ;
    sn:hasPosition "0,5,0" ;
    sn:memberOf sn:designers ;
    sn:hasColor "0xF5A623" ;
    sn:hasHeight "5" .

sn:researchers a sn:Community ;
    foaf:name "Researchers" ;
    sn:hasPosition "0,10,0" ;
    sn:memberOf sn:researchers ;
    sn:hasColor "0x7ED321" ;
    sn:hasHeight "10" .

sn:managers a sn:Community ;
    foaf:name "Managers" ;
    sn:hasPosition "0,15,0" ;
    sn:memberOf sn:managers ;
    sn:hasColor "0xD0021B" ;
    sn:hasHeight "15" .

